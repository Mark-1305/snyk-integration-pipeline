pipeline{

    agent any
    stages{
        stage('Git Checkout'){
            steps{
                git branch: 'main', url: 'https://github.com/Mark-1305/counter-app.git'
            }
        }
        stage('Snyk-Test'){
            steps{
                script{
                    snykSecurity failOnError: false, monitorProjectOnBuild: false, severity: 'high', snykInstallation: 'snyk_test', snykTokenId: 'snyk-api-token'
                    }
                 }
           }      

    }
}
